<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.covid19symptonrecorder">
 <uses-permission android:name="android.permission.CALL_PHONE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.COVID19SYMPTONRECORDER">
        <activity android:name=".FutherDetails"></activity>
        <activity android:name=".Medicines" />
        <activity android:name=".HealthServices" />
        <activity android:name=".Emergency" />
        <activity android:name=".HealthTopic" />
        <activity android:name=".CheckSymptom" />
        <activity android:name=".CompareCovid" />
        <activity android:name=".ActivityLogin" />
        <activity android:name=".HomeScreen" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>